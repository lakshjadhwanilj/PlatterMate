<!-- Card -->
<div class='card' style='width: 18rem;'>
    <img src='<%= recipe.recipe.image %>' alt='<%= recipe.recipe.label %>'>
    <div class='card-body'>
        <h5 class='card-title'><%= recipe.recipe.label %></h5>
        <p class='card-text'>This is a recipe for <%= recipe.recipe.label %></p>
    </div>
</div>